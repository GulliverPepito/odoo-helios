        <!-- Journal Sequences  -->

        <record id="cl_sequence_gen_inve" model="ir.sequence">
            <field name="name">INVE Diario Inventarios (Secuencia)</field>
            <field name="prefix">INVE/%(range_year)s/</field>
            <!--<field eval="no_gap" name="implementation"/>-->
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="1" name="company_id"/>
